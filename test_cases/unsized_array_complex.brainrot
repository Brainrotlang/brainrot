skibidi main {
    rizz arr[] = {1, 2, 3, 4, 5};
    rizz n = maxxing(arr) / maxxing(arr[0]);

    ðŸš½ compute sum, min, max; reverse into another array; then print
    rizz sum = 0;
    rizz min = 2147483647;
    rizz max = -2147483648;

    flex (rizz i = 0; i < n; i = i + 1) {
        sum = sum + arr[i];
        edgy (arr[i] < min) { min = arr[i]; }
        edgy (arr[i] > max) { max = arr[i]; }
    }

    rizz rev[] = {0, 0, 0, 0, 0};
    flex (rizz i = 0; i < n; i = i + 1) {
